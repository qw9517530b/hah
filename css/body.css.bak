body{
	margin:0;
	margin:0;
	font-size: 100%;
	font-family: 'Open Sans', sans-serif;
	font-size: 14px;
	color: #999999;
}


p,h1,h2,h3,h4,h5,h6,ul,ol,dl,dd{
	margin:0;
	margin:0;
	list-style:none;
}

a{
	font-size:18px;
	text-decoration:none;
}

a:hover:{
	font-weight:bold;
}

img{
	vertical-align:middle;
}
.lf{
	float:left;
}

.rf{
	float:right;
}

.clear{
	content:"";
	display:block;
	clear:both;
}

#header1{
	width:100%;
	height:120px;
	margin:auto;
	margin-bottom:100px;
}

#footer1{
	width:100%;
	height:400px;
	margin:auto;	
	margin-bottom:50px;
}